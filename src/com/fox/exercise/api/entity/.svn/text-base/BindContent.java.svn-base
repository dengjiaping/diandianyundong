package com.fox.exercise.api.entity;


public class BindContent {
    public int getLikesText() {
        return likesText;
    }

    public void setLikesText(int likesText) {
        this.likesText = likesText;
    }

    public boolean isLikesImage() {
        return likesImage;
    }

    public void setLikesImage(boolean likesImage) {
        this.likesImage = likesImage;
    }

    public int getUserWavTimes() {
        return userWavTimes;
    }

    public void setUserWavTimes(int userWavTimes) {
        this.userWavTimes = userWavTimes;
    }

    public int getcCount() {
        return cCount;
    }

    public void setcCount(int cCount) {
        this.cCount = cCount;
    }

    private int likesText;
    private boolean likesImage;
    private int userWavTimes;
    private int cCount;

    public BindContent(int likesText, boolean likesImage, int userWavTimes,
                       int cCount) {
        super();
        this.likesText = likesText;
        this.likesImage = likesImage;
        this.userWavTimes = userWavTimes;
        this.cCount = cCount;
    }

    @Override
    public String toString() {
        return "BindContent [likesText=" + likesText + ", likesImage="
                + likesImage + ", userWavTimes=" + userWavTimes + ", cCount="
                + cCount + "]";
    }

}
