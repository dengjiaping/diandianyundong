package com.fox.exercise.login;

public class OriginalLoginInfos {
    private String name = "";
    private String pwd = "";
    private String sex = "";
    private int loginFinish = 0;
    private int loginFail = 0;

    public OriginalLoginInfos(String name, String pwd, String sex, int loginFinish, int loginFail) {
        this.name = name;
        this.pwd = pwd;
        this.sex = sex;
        this.loginFail = loginFail;
        this.loginFinish = loginFinish;
    }

    public int getLoginFinish() {
        return loginFinish;
    }

    public int getLoginFail() {
        return loginFail;
    }

    public String getName() {
        return name;
    }

    public String getPwd() {
        return pwd;
    }

    public String getSex() {
        return sex;
    }
}
