    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10dip" />

            <gradient
                android:angle="90"
                android:endColor="#969696"
                android:startColor="#F0F0F0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dip" />

                <gradient
                    android:angle="90"
                    android:endColor="#cb645d"
                    android:startColor="#e26b66" />
            </shape>
        </clip>
    </item>

